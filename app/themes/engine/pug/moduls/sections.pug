mixin breadcrumbs(arr)
    .breadcrumbs        
        .container
            .breadcrumbs-nav.d-flex.flex-wrap.text-nowrap
                -for(let i = 0; i < arr.length; i++)
                    if i === arr.length - 1                        
                        span.breadcrumbs-nav-item.breadcrumbs-nav-item-disabled
                            span= arr[i]['title']
                    else                                                
                        a(href= arr[i]['href']+ '.html' title= arr[i]['title']).breadcrumbs-nav-item
                            span= arr[i]['title']
                            i.icon-chevron.fs-16
            
